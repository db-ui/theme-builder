{
  "name": "theme-builder",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite --open",
    "generate:parse-options": "node scripts/generateParseOptions.js",
    "generate:tailwind": "tailwindcss -i ./src/tailwind.css -o ./src/components/Editor/tailwind.generated.css --full --minify",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "postbuild": "ncp dist/index.html dist/404.html",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {}\"",
    "preview": "vite preview"
  },
  "dependencies": {
    "@db-ui/react-components": "^0.0.61",
    "@redux-devtools/extension": "^3.3.0",
    "@types/node": "^20.10.4",
    "ace-builds": "1.32.2",
    "apca-w3": "^0.1.9",
    "chroma-js": "^2.4.2",
    "hsluv": "^1.0.1",
    "html-react-parser": "^5.0.7",
    "husky": "8.0.3",
    "i18next": "23.7.9",
    "i18next-browser-languagedetector": "7.2.0",
    "i18next-http-backend": "2.4.2",
    "jszip": "^3.10.1",
    "prettier": "3.1.1",
    "react": "^18.2.0",
    "react-ace": "10.1.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-i18next": "13.5.0",
    "react-router-dom": "6.20.1",
    "traverse": "^0.6.7",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/apca-w3": "^0.1.3",
    "@types/chroma-js": "^2.4.3",
    "@types/prettier": "^3.0.0",
    "@types/react": "^18.2.45",
    "@types/react-color": "^3.0.10",
    "@types/react-dom": "^18.2.17",
    "@types/traverse": "^0.6.35",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "ncp": "^2.0.0",
    "postcss": "^8.4.32",
    "sass": "^1.69.5",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3",
    "vite": "^5.0.9"
  }
}
